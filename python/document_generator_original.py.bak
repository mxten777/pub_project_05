    def export_to_word(self, content: str, filename: str):
        """
        Word ë¬¸ì„œë¡œ ë‚´ë³´ë‚´ê¸°
        
        Args:
            content: ë¬¸ì„œ ë‚´ìš©
            filename: ì €ì¥í•  íŒŒì¼ëª…
        """
        print(f"ğŸ“„ Word ë¬¸ì„œ ìƒì„± ì¤‘: {filename}")
        
        doc = Document()
        
        # ì œëª© ì¶”ê°€
        title = doc.add_heading('ì…ì°° ë¬¸ì„œ', 0)
        title.alignment = WD_ALIGN_PARAGRAPH.CENTER
        
        # ë‚´ìš© ì¶”ê°€
        for line in content.split('\n'):
            if line.strip():
                p = doc.add_paragraph(line)
                p.style.font.size = Pt(11)
        
        # ì €ì¥
        doc.save(f'output/{filename}.docx')
        print(f"âœ… Word ë¬¸ì„œ ì €ì¥ ì™„ë£Œ: output/{filename}.docx")
    
    def export_to_pdf(self, content: str, filename: str):
        """
        PDF ë¬¸ì„œë¡œ ë‚´ë³´ë‚´ê¸°
        
        Args:
            content: ë¬¸ì„œ ë‚´ìš©
            filename: ì €ì¥í•  íŒŒì¼ëª…
        """
        print(f"ğŸ“„ PDF ë¬¸ì„œ ìƒì„± ì¤‘: {filename}")
        
        c = canvas.Canvas(f'output/{filename}.pdf', pagesize=A4)
        width, height = A4
        
        # ì œëª©
        c.setFont("Helvetica-Bold", 16)
        c.drawString(50, height - 50, "Bid Document")
        
        # ë‚´ìš©
        c.setFont("Helvetica", 10)
        y = height - 100
        
        for line in content.split('\n'):
            if y < 50:  # í˜ì´ì§€ ë„˜ê¹€
                c.showPage()
                c.setFont("Helvetica", 10)
                y = height - 50
            
            c.drawString(50, y, line[:80])  # í•œ ì¤„ì— 80ìê¹Œì§€
            y -= 15
        
        c.save()
        print(f"âœ… PDF ë¬¸ì„œ ì €ì¥ ì™„ë£Œ: output/{filename}.pdf")


